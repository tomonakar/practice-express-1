    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.4",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
